{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "next"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "rules": {
    "semi": "error",
    "constructor-super": "error",
    "for-direction": "error",
    //"arrow-body-style": "warn",
    "camelcase": "error",
    "curly": "warn",
    "eqeqeq": "warn",
    "eol-last": "error"
  }
}
